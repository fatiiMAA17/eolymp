n, m = map(int, input().split())

# DP massivini yaradırıq
dp = [ [0]*(m+1) for _ in range(n+1) ]
dp[0][0] = 1  # 0 rəqəm, cəmi 0 olan ədədlər

for i in range(1, n+1):
    for s in range(0, m+1):
        # ilk rəqəm
        start_d = 1 if i == 1 else 0
        for d in range(start_d, 10):
            if s - d >= 0:
                dp[i][s] += dp[i-1][s-d]

print(dp[n][m])
