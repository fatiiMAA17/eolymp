import math

a, b, c = map(int, input().split())

D = b * b - 4 * a * c

if D < 0:
    print("No roots")
elif D == 0:
    x = -b // (2 * a)
    print("One root:", x)
else:
    sqrt_D = int(math.sqrt(D))
    x1 = (-b - sqrt_D) // (2 * a)
    x2 = (-b + sqrt_D) // (2 * a)
    if x1 > x2:
        x1, x2 = x2, x1
    print("Two roots:", x1, x2)
